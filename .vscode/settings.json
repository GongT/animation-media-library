{
	"files.associations": {
		"*.json": "jsonc",
	},
	"search.exclude": {
		"**/temp": true,
		"**/node_modules": true,
		"**/lib": true,
		"**/dist": true,
		"pnpm-lock.yaml": true,
		"**/*.generated.ts": true,
	},
	"typescript.tsdk": "./node_modules/typescript/lib",
	"biome.lsp.bin": "./node_modules/@biomejs/biome/bin/biome",
	"[json][json5][jsonc][javascript][typescript][typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome",
	},
	"cSpell.words": ["bgmtv"],
	"remote.portsAttributes": {
		"8888": {
			"label": "adminer",
			"protocol": "http",
			"onAutoForward": "silent",
		},
	}
}
