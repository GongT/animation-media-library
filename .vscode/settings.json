{
	"files.associations": {
		"*.json": "jsonc"
	},
	"search.exclude": {
		"**/temp": true,
		"**/node_modules/**/*": true,
		"*/*/lib": true,
		"*/*/.rush": true,
		"**/*.api.json": true,
		"**/*.api.md": true,
		"**/*.generated.d.ts": true,
		"**/*.generated.ts": true
	},
	"prettier.prettierPath": "common/temp/node_modules/prettier",
	"prettier.requireConfig": true,
	"typescript.tsdk": "common/temp/node_modules/typescript/lib",
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json5]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jacascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
